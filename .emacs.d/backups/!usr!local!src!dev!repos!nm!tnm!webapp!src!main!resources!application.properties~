# How to connect to the main server
tnm.mainserver.hostname = localhost
tnm.mainserver.rmiPort = 1099

server.port = 8081
server.ssl.enabled = false

# How to connect to SDN controller
sdn.controller.ip = localhost
sdn.controller.port = 8181
sdn.enabled = false

# DNA toggles
# Example of ws url: http://10.220.27.215:9999/mtosi/
dna.mtosi.url = Undefined
dna.mtosi.username = Undefined
dna.mtosi.password = Undefined

# RabbitMQ
spring.rabbitmq.username = tnm
spring.rabbitmq.password = transmode
spring.rabbitmq.virtual-host = /tnm
amqp.enabled = false
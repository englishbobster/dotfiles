defmodule Mod1 do
  def doit(value, :a) do
    value * 10
  end

end

defmodule Mod2 do
  def doit(value, :b) do
    value / 10
  end

end

defmodule DoStuff do
  import Mod1
  import Mod2

  def it_happens_here(ls) do
    ls
    |> Enum.map(fn elem -> doit(elem, :a) end)
    |> Enum.map(fn elem -> doit(elem, :b) end)
    |> IO.inspect
  end
end

DoStuff.it_happend_here([10, 20, 30, 40, 50, 60])

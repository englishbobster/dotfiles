defmodule StockingStuffer do
  def secret_key do
    "ckczppom"
  end

end

ExUnit.start([trace: true])

defmodule StockingStufferTest do
  use ExUnit.Case

  test "lowest number that gives 5 zeros" do
    assert lowest_mined_for("abcdef") == 609043
    assert lowest_mined_for("pqrstuv") == 1048970
  end

end

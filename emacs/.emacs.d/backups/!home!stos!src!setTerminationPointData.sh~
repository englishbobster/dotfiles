#!/bin/bash

#TARGET_WS_ADDR="http://172.16.15.122:8080"
TARGET_WS_ADDR="http://localhost:8080"
ENTRY_POINT="/soap/mtosi/TerminationPointControl"

curl --header "Content-Type: text/xml;charset=UTF-8" --header "SOAPAction: setTerminationPointData" --data @set_terminationPointDataReq.xml "$TARGET_WS_ADDR$ENTRY_POINT" --connect-timeout 10 &

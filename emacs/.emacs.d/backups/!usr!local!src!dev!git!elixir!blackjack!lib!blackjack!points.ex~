defmodule Blackjack.Points do
	defp points do
		%{ace_high: 11,
			ace_low: 1,
			two: 2,
			three: 3,
			four: 4,
			five: 5,
			six: 6,
			seven: 7,
			eight: 8,
			nine: 9,
			ten: 10,
			jack: 10,
			queen: 10,
			king: 10
			}
	end

	def calculate_points(hand) do
		hand |> Enum.map(fn -> 0 end)
	end
	
end
